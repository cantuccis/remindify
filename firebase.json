{
  "hosting": {
    "site": "remindify-axny-dev",
    "public": "build/web"
  },
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    },
    "auth": {
      "port": 9099
    },
    "hosting": {
      "port": 5000
    }
  }
}
